{
    "version": 2,
    "builds": [
        {
            "src": "build/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/build/static/$1"
        },
        { "src": "/src/(.*)", "dest": "/src/$1" },
        { "src": "/(.*)", "dest": "/server/server.js", 
        "methods": [
            "GET",
            "POST",
            "PUT",
            "PATCH",
            "DELETE",
            "OPTIONS"]}
        ]                 
  }